+  public class DelNullDir {
+        public void ShowDir(File f){
+            for (File f1:f.listFiles()){
+                if(f1.isDirectory()){
+                    ShowDir(f1);
+                    //一直递归到最后的目录
+                    if(f1.listFiles().length==0){
+                        //如果是文件夹里面没有文件证明是空文件，进行删除
+                        f1.delete();
+                    }
+                }
+            }
+        }
+    }
+
+}
